name: {{pubName}}
version: {{pubVersion}}
description: {{pubDescription}}
homepage: {{pubHomepage}}
{{#pubRepository}}
repository: {{.}}
{{/pubRepository}}
{{#pubPublishTo}}
publish_to: {{.}}
{{/pubPublishTo}}


environment:
  sdk: '>={{#useJsonSerializable}}3.5.0{{/useJsonSerializable}}{{^useJsonSerializable}}{{#useFreezed}}3.5.0{{/useFreezed}}{{^useFreezed}}2.18.0{{/useFreezed}}{{/useJsonSerializable}} <4.0.0'

dependencies:
  dio: '^5.7.0'
{{#useBuiltValue}}
  one_of: '>=1.5.0 <2.0.0'
  one_of_serializer: '>=1.5.0 <2.0.0'
  built_value: '>=8.4.0 <9.0.0'
  built_collection: '>=5.1.1 <6.0.0'
{{/useBuiltValue}}
{{#useEquatable}}
  equatable: '^2.0.7'
{{/useEquatable}}
{{#useJsonSerializable}}
  {{^skipCopyWith}}
  copy_with_extension: '^7.1.0'
  {{/skipCopyWith}}
  json_annotation: '^4.9.0'
{{/useJsonSerializable}}
{{#useFreezed}}
  freezed_annotation: '>=2.4.0 <3.0.0'
{{/useFreezed}}
{{#useResultDart}}
  result_dart: '^2.1.1'
{{/useResultDart}}
{{#useDateLibTimeMachine}}
  time_machine: ^0.9.17
{{/useDateLibTimeMachine}}

dev_dependencies:
{{#useBuiltValue}}
  built_value_generator: '>=8.4.0 <9.0.0'
  build_runner: any
{{/useBuiltValue}}
{{#useJsonSerializable}}
  build_runner: any
  {{^skipCopyWith}}
  copy_with_extension_gen: ^7.1.0
  {{/skipCopyWith}}
  json_serializable: '^6.9.3'
{{/useJsonSerializable}}
{{#useFreezed}}
  build_runner: any
  freezed: '>=2.4.0 <3.0.0'
  json_serializable: '^6.9.3'
{{/useFreezed}}
  test: '^1.16.0'
